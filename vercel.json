{
  "headers": [
    {
      "source": "/resume/",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/resume",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/staticfiles/$1"
    },
    {
      "source": "/resume",
      "destination": "/resume"
    },
    {
      "source": "/:path*",
      "destination": "portfolio/wsgi.py"
    }
  ]
}
